@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewMembers</title>
    @Html.Partial("PartialPage")
    <link rel="shortcut icon" href="~/Content/images/49.png">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
</head>

<body style="font-family:Algerian ;color:white;overflow-x:hidden ;background-image:url('/Content/images/black.jpg');background-repeat:no-repeat;background-size:cover;">
    <div class="container-fluid">

        @Html.Partial("AdminPartialHeader")

        <h2>List of all Contacters <br /> <br /> </h2>

        <div class="table-responsive">
            <table class="table" style="background: #5499C7; padding: 10px; width: 95%; border-radius: 10px; margin-left: 30px; font-size: 20px;">

               
                <tr>
                    <td style="padding-left:20px"><br />&nbsp; ID <br /><br /></td>
                    <td style="padding-left:20px"><br />&nbsp; Name <br /><br /></td>
                    <td style="padding-left:20px"><br />&nbsp;E_mail<br /><br /></td>
                    <td style="padding-left:20px"><br />&nbsp;Subject <br /><br /></td>
                    <td style="padding-left:20px"><br />&nbsp;Message <br /><br /></td>



                </tr> 

                <tbody>

                    @foreach (var s in Model)
                    {
                        <tr style="border:1px solid black">

                            <td style="padding: 20px; padding-left: 70px; font-family: 'Times New Roman'"> @s.contacter_Id</td>
                            <td style="padding: 20px; padding-left: 70px; font-family: 'Times New Roman'"> @s.con_name</td>
                            <td style="padding: 20px; padding-left: 70px; font-family: 'Times New Roman'"> @s.e_mail</td>
                            <td style="padding: 20px; padding-left: 70px; font-family: 'Times New Roman' "> @s.subject</td>

                            <td style="padding: 50px; padding-left: 70px; font-family: 'Times New Roman'">@s.message</td>

                            
                            <td>
                                &nbsp; &nbsp; &nbsp; <a href="/Admin/Delete1/@s.contacter_Id" style="color:black">   Delete </a>
                                &nbsp; &nbsp; &nbsp;
                            </td>
                        </tr>
                    }


                </tbody>
            </table>
        </div>
        <!---->
        <br /><br />
    </div>





    <script src="~/js/bootstrap.js" type="text/javascript"></script>

</body>

</html>

 